{
  "manifest_version": 3,

  "name": "Your Chromium Version",
  "description": "Check Your Chromium Version and Update.",
  "version": "0.1.0",

  "action": {
    "default_icon": "icon.png",
    "default_popup": "popup.html"
  },

  "icons": {
  },

  "content_scripts": [
    {
      "matches": ["*://*/*"],
      "js": [
        "content.js"
      ]
    }
  ],

  "background": {
    "service_worker": "background.js"
  },

  "permissions": [
    "background"
  ]
}